            });
        }

        function toggleBackToTop() {
            const backToTopBtn = document.querySelector('.back-to-top');
            if (window.pageYOffset > 300) {
                backToTopBtn.classList.add('show');
            } else {
                backToTopBtn.classList.remove('show');
            }
        }

        // Initialize on page load
        document.addEventListener('DOMContentLoaded', function () {
            initTheme();
            
            // Back to top button
            window.addEventListener('scroll', toggleBackToTop);
        
            // Theme toggle click event
            const themeToggle = document.getElementById('themeToggle');
            if (themeToggle) {
                themeToggle.addEventListener('click', function (e) {
                    e.preventDefault();
                    toggleTheme();
                });
            }
            
            // Hamburger menu functionality
            const hamburgerMenu = document.getElementById('hamburgerMenu');
            const mobileMenu = document.getElementById('mobileMenu');
            const menuOverlay = document.getElementById('menuOverlay');
            
            function toggleMenu() {
                hamburgerMenu.classList.toggle('active');
                mobileMenu.classList.toggle('active');
                menuOverlay.classList.toggle('active');
                document.body.style.overflow = mobileMenu.classList.contains('active') ? 'hidden' : '';
            }
            
            if (hamburgerMenu) {
                hamburgerMenu.addEventListener('click', toggleMenu);
            }
            
            if (menuOverlay) {
                menuOverlay.addEventListener('click', toggleMenu);
            }
            
        });

        // Form submissions
        document.addEventListener('submit', function (e) {
            if (e.target.name === 'newsletter') {
                e.preventDefault();
                alert('Thank you for subscribing! You will receive updates about new mysteries.');
            }
        });
    
    // Event listener for scrollToTop()
    document.addEventListener('DOMContentLoaded', function() {
        const elements = document.querySelectorAll('html');
        elements.forEach(element => {
            if (element.textContent.includes('scrollToTop') || element.getAttribute('class')?.includes('scrolltotop')) {
                element.addEventListener('click', scrollToTop());
            }
        });
    });</script>

    <!-- Common JS (Header/Footer loader) -->
    <script src="/common.js"></script>

</body>
</html>
